{
    "name": "zhukovsergei/factorio-blueprint-parser",
    "description": "A simple parser for Factorio blueprints.",
    "type": "library",
    "require": {
        "php": "^7.4",
        "symfony/serializer": "4.4.x-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Zhukov Sergei",
            "email": "alivesystemsonly@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "scripts": {
        "lint": "phplint",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "psalm": "psalm",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-html var/coverage",
        "app": "php bin/app.php --ansi"
    },
    "require-dev": {
        "phpunit/phpunit": "9",
        "squizlabs/php_codesniffer": "^3.5",
        "overtrue/phplint": "^1.2",
        "vimeo/psalm": "^3.9"
    }
}
